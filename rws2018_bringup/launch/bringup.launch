  <node pkg="rws2018_referee" type="referee.py" name="referee" output="screen"/>
  <!--Launch the killer-->
  <node pkg="rws2018_referee" type="killer.py" name="killer"/>
  <node pkg="rws2018_referee" type="draw_teams" name="draw_teams"/>
