  <rosparam command="load" file="$(find rws2018_bringup)/params/default.yaml"/>
  <!--Launch the visualization-->
  <node pkg="rviz" type="rviz" name="rviz" args="--display-config $(find rws2018_bringup)/config/rviz.rviz" required="true"/>
